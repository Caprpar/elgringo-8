-- init --
function _init() 

	player = {
	 x = 63,
	 y = 63,
	 dx = 1,
	 dy = 1,
	 sp = 1,
	 vel = 5,
	 running = false,
	 flipped = false
	}

end

function _update() 
 if (btn(⬅️)) then 
  player.x-=player.vel
  player.flipped = true
 end
 
 if (btn(➡️)) then
  player.x+=player.vel
  player.flipped = false
	end
	
 if (btn(⬆️)) then
  player.y-=player.vel
	end

 if (btn(⬇️)) then
  player.y+=player.vel
	end

end

function _draw() 
	cls()
	spr(
		player.sp, 
		player.x, 
		player.y,
		1,
		1,
		player.flipped)
 
end
